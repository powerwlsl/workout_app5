<h1><%= @followed_friend.full_name %>'s Exercises</h1>
<table class="table table-striped">
	<thead>
		<tr>
			<th>Duration</th>
			<th>Details</th>
			<th>Date</th>
			<th colspan="3"></th>
		</tr>
	</thead>

	<tbody>
		<% @exercises.each do |exercise| %>
			<tr>
				<td><%= exercise.duration %></td>
				<td><%= exercise.detail %></td>
				<td><%= exercise.date %></td>
			</tr>
		<% end %>
	</tbody>
</table>

<div>
 	<%= content_tag :div, "",id: "chart", data: { workouts: @exercises } %>
</div>